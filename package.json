{
  "name": "@travi/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/scaffolder-sub-command",
  "bugs": "https://github.com/travi/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@travi/scaffolder-sub-command",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:wip": "run-s 'test:integration:base -- --tags \"@wip and not @skip\"'",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @skip and not @wip\"'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.8.2",
    "@travi/eslint-config-travi": "^1.6.18",
    "babel-preset-travi": "1.4.0",
    "babel-register": "6.26.0",
    "bdd-stdin": "^0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.1.0",
    "commitlint-config-travi": "1.2.31",
    "cucumber": "5.0.2",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.0.0-rc.8",
    "mocha": "5.2.0",
    "mock-fs": "^4.6.0",
    "mz": "^2.7.0",
    "npm-run-all": "4.1.3",
    "nyc": "^13.0.1",
    "rimraf": "2.6.2",
    "rollup": "0.66.6",
    "sinon": "^6.1.5"
  },
  "dependencies": {
    "@travi/github-scaffolder": "1.0.0",
    "@travi/gitlab-scaffolder": "1.0.0",
    "@travi/javascript-scaffolder": "3.13.3",
    "@travi/project-scaffolder": "5.0.2",
    "@travi/travis-scaffolder-javascript": "1.4.4"
  }
}
