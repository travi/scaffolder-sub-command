{
  "name": "@travi/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/scaffolder-sub-command",
  "bugs": "https://github.com/travi/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@travi/scaffolder-sub-command",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @wip\"'",
    "test:integration:base": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --tags @wip'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@travi/any": "1.8.5",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config-travi": "^1.6.18",
    "bdd-stdin": "^0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.2.0",
    "commitlint-config-travi": "1.2.40",
    "cucumber": "5.0.2",
    "cz-conventional-changelog": "2.1.0",
    "debug": "^4.1.1",
    "http-status-codes": "^1.3.0",
    "husky": "^1.0.0-rc.8",
    "mocha": "6.0.2",
    "mock-fs": "^4.6.0",
    "mz": "^2.7.0",
    "nock": "^10.0.6",
    "npm-run-all": "4.1.5",
    "nyc": "^13.0.1",
    "rimraf": "2.6.3",
    "rollup": "1.2.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.2.3"
  },
  "dependencies": {
    "@travi/github-scaffolder": "3.0.3-alpha.6",
    "@travi/gitlab-scaffolder": "1.0.1",
    "@travi/javascript-scaffolder": "5.0.0",
    "@travi/netlify-scaffolder": "1.1.0",
    "@travi/node-app-engine-standard-scaffolder": "1.0.0-alpha.1",
    "@travi/project-scaffolder": "6.0.2",
    "@travi/travis-scaffolder-javascript": "1.10.0"
  }
}
