{
  "name": "@travi/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/scaffolder-sub-command",
  "bugs": "https://github.com/travi/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@travi/scaffolder-sub-command",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @wip\"'",
    "test:integration:base": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --tags @wip'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.6.0",
    "@travi/any": "2.0.4",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config-travi": "^1.8.5",
    "bdd-stdin": "^0.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.6.1",
    "commitlint-config-travi": "1.3.4",
    "cucumber": "^5.1.0",
    "cz-conventional-changelog": "3.0.2",
    "debug": "^4.1.1",
    "http-status-codes": "^1.3.2",
    "husky": "^3.0.5",
    "mocha": "6.2.0",
    "mock-fs": "^4.10.1",
    "mz": "^2.7.0",
    "nock": "^11.3.4",
    "npm-run-all": "4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "rollup": "1.21.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.4.2",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@form8ion/hapi-scaffolder": "1.2.0",
    "@form8ion/ruby-scaffolder": "1.0.0",
    "@travi/circle-scaffolder-javascript": "1.2.1",
    "@travi/github-scaffolder": "4.1.2",
    "@travi/gitlab-scaffolder": "1.0.2",
    "@travi/javascript-scaffolder": "7.9.1",
    "@travi/netlify-scaffolder": "1.2.0",
    "@travi/node-app-engine-standard-scaffolder": "1.0.0-alpha.1",
    "@travi/project-scaffolder": "7.0.13",
    "@travi/shell-scaffolder": "1.1.1",
    "@travi/spectacle-scaffolder": "2.0.0",
    "@travi/travis-scaffolder-javascript": "1.12.0",
    "@travi/travis-scaffolder-shell": "1.0.0-alpha.4"
  }
}
